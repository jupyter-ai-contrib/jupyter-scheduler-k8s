{
  "name": "jupyter-scheduler-k8s",
  "version": "0.1.0",
  "description": "Kubernetes backend and UI extensions for jupyter-scheduler",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "kubernetes"
  ],
  "homepage": "https://github.com/jupyter/jupyter-scheduler-k8s",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Jupyter Development Team"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyter/jupyter-scheduler-k8s.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:prod": "jlpm clean && jlpm build:lib && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyter_scheduler_k8s/labextension",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension",
    "install:extension": "jlpm build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@mui/material": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@jupyterlab/scheduler": "file:../jupyter-scheduler",
    "@types/react": "^18.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@jupyterlab/scheduler": "^2.11.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyter_scheduler_k8s"
        }
      }
    },
    "extension": true,
    "outputDir": "jupyter_scheduler_k8s/labextension",
    "disabledExtensions": [
      "@jupyterlab/scheduler:IAdvancedOptions"
    ],
    "sharedPackages": {
      "@jupyterlab/scheduler": {
        "singleton": true,
        "bundled": false
      },
      "@mui/material": {
        "singleton": true
      },
      "@mui/system": {
        "singleton": true
      },
      "react": {
        "singleton": true
      },
      "react-dom": {
        "singleton": true
      }
    }
  }
}
